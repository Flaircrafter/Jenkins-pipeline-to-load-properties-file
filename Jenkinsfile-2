pipeline {

  agent any
 
  environment {
    ARTIFACTORY = credentials("testing")
  }

  options {
    timestamps()
    disableConcurrentBuilds()
  }

  stages {
    stage ('Load Properties file') {
      steps {
        script {
          def helper = load "buildHelper.groovy"
          def vars = helper.buildvars()
          
          echo "======= $vars"
          
          echo "appname ========= ${vars.'appname'}"
        }
      }
    }
  }
}
